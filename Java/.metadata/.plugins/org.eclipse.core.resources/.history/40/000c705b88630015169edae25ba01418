

import kareltherobot.*;

public class Newspaper implements Directions {

	public static void task() {
		UrRobot Framess = new UrRobot(1,1,North,10); 
		
		
		Framess.move();
		Framess.move();
		Framess.turnLeft();
		Framess.turnLeft();
		Framess.turnLeft();
		Framess.move();
		Framess.move();
		Framess.pickBeeper();
		Framess.turnLeft();
		Framess.move();
		Framess.turnLeft();
		Framess.move();
		Framess.turnLeft();
		Framess.turnLeft();
		Framess.turnLeft();
		Framess.move();
		Framess.turnLeft();
		Framess.turnLeft();
		Framess.turnLeft();
		Framess.move();
		Framess.move();
		Framess.move();
	}

	// Main entry point
	static public void main(String[] args) {
		World.setDelay(15);
		 World.readWorld("worlds/world25.txt");
		World.setVisible();
		World.setSize(11,10);
		task();
	}
}

